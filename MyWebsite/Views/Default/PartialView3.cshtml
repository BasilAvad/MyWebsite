@using MyWebsite.Models.Sınıflar
@model List<RecentProject>  
<div class="padding">
    <h4>Top 10 Projects </h4>
    <ul class="list list-pad">
       @{ int i = 0;}
        @foreach (var x in Model)
        {


        <li class="list-bg"><span>@{i++; } @i.</span><a href="/RecentProject/BlogDetay/@x.Id">@x.Baslik</a></li>
        }
    </ul>
    <a href="/RecentProject/Index/" class="link1">See all projects</a>
</div>
